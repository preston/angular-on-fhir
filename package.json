{
    "name": "angular-on-fhir",
    "version": "1.1.0",
    "description": "Angular base project for SMART and other FHIR-based UIs.",
    "license": "Apache-2.0",
    "repository": "git@github.com:preston/angular-on-fhir.git",
    "scripts": {
        "ng": "ng",
        "start": "envsubst < src/assets/runtime-configuration.template.js > src/assets/runtime-configuration.js && ng serve",
        "build": "ng build",
        "watch": "envsubst < src/assets/runtime-configuration.template.js > src/assets/runtime-configuration.js && ng build --watch --configuration development",
        "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/angular-on-fhir/stats.json -m static",
        "test": "ng test"
    },
    "author": "Preston Lee",
    "dependencies": {
        "@angular/animations": ">=14.2.4",
        "@angular/common": ">=14.2.4",
        "@angular/compiler": ">=14.2.4",
        "@angular/core": ">=14.2.4",
        "@angular/forms": ">=14.2.4",
        "@angular/localize": ">=14.2.4",
        "@angular/platform-browser": ">=14.2.4",
        "@angular/platform-browser-dynamic": ">=14.2.4",
        "@angular/router": ">=14.2.4",
        "@graphitehealth/sdk": "file:../graphite-sdk",
        "@ng-bootstrap/ng-bootstrap": "^13.0.0",
        "@opentelemetry/exporter-metrics-otlp-http": "^0.33.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.33.0",
        "@opentelemetry/instrumentation-http": "^0.33.0",
        "@opentelemetry/sdk-trace-base": "^1.7.0",
        "@opentelemetry/sdk-trace-web": "^1.7.0",
        "jquery": "^3.6.1",
        "@popperjs/core": "^2.11.6",
        "bootstrap": "^5.2.1",
        "bootstrap-icons": "^1.9.1",
        "fhir-kit-client": "^1.9.1",
        "fhirclient": "^2.5.2",
        "moment": "^2.29.4",
        "ngx-moment": "^6.0.2",
        "rxjs": "^7.5.7",
        "zone.js": "^0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": ">=14.2.4",
        "@angular/cli": ">=14.2.4",
        "@angular/compiler-cli": ">=14.2.4",
        "@types/fhir": "^0.0.35",
        "@types/node": ">=18.8.0",
        "typescript": "^4.8.4",
        "webpack-bundle-analyzer": "^4.6.1"
    }
}