{
    "name": "angular-on-fhir",
    "version": "1.2.0",
    "description": "Angular base project for SMART and other FHIR-based UIs.",
    "license": "Apache-2.0",
    "repository": "git@github.com:preston/angular-on-fhir.git",
    "scripts": {
        "ng": "ng",
        "start": "envsubst < src/assets/configuration.template.js > src/assets/configuration.js && ng serve",
        "build": "ng build",
        "watch": "envsubst < src/assets/configuration.template.js > src/assets/configuration.js && ng build --watch --configuration development",
        "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/angular-on-fhir/stats.json -m static",
        "test": "ng test"
    },
    "author": "Preston Lee",
    "dependencies": {
        "@angular/animations": ">=16.0.4",
        "@angular/common": ">=16.0.4",
        "@angular/compiler": ">=16.0.4",
        "@angular/core": ">=16.0.4",
        "@angular/forms": ">=16.0.4",
        "@angular/localize": ">=16.0.4",
        "@angular/platform-browser": ">=16.0.4",
        "@angular/platform-browser-dynamic": ">=16.0.4",
        "@angular/router": ">=16.0.4",
        "@ng-bootstrap/ng-bootstrap": "^15.0.1",
        "jquery": "^3.7.0",
        "@popperjs/core": "^2.11.8",
        "bootstrap": "^5.3.0",
        "bootstrap-icons": "^1.10.5",
        "fhir-kit-client": "^1.9.2",
        "fhirclient": "^2.5.2",
        "moment": "^2.29.4",
        "ngx-moment": "^6.0.2",
        "rxjs": "^7.8.1",
        "zone.js": "^0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": ">=16.0.4",
        "@angular/cli": ">=16.0.4",
        "@angular/compiler-cli": ">=16.0.4",
        "@types/fhir": "^0.0.37",
        "@types/node": ">=20.2.5",
        "typescript": "< 5.1",
        "webpack-bundle-analyzer": "^4.9.0"
    }
}