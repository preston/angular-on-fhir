{
    "name": "angular-on-fhir",
    "version": "1.2.0",
    "description": "Angular base project for SMART and other FHIR-based UIs.",
    "license": "Apache-2.0",
    "repository": "git@github.com:preston/angular-on-fhir.git",
    "scripts": {
        "ng": "ng",
        "start": "envsubst < src/assets/runtime-configuration.template.js > src/assets/runtime-configuration.js && ng serve",
        "build": "ng build",
        "watch": "envsubst < src/assets/runtime-configuration.template.js > src/assets/runtime-configuration.js && ng serve --watch --configuration development",
        "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/angular-on-fhir/stats.json -m static",
        "test": "ng test"
    },
    "author": "Preston Lee",
    "dependencies": {
        "@angular/animations": ">=15.0.4",
        "@angular/common": ">=15.0.4",
        "@angular/compiler": ">=15.0.4",
        "@angular/core": ">=15.0.4",
        "@angular/forms": ">=15.0.4",
        "@angular/localize": ">=15.0.4",
        "@angular/platform-browser": ">=15.0.4",
        "@angular/platform-browser-dynamic": ">=15.0.4",
        "@angular/router": ">=15.0.4",
        "@ng-bootstrap/ng-bootstrap": "^14.0.0",
        "jquery": "^3.6.2",
        "@popperjs/core": "^2.11.6",
        "bootstrap": "^5.2.3",
        "bootstrap-icons": "^1.10.2",
        "fhir-kit-client": "^1.9.2",
        "fhirclient": "^2.5.2",
        "moment": "^2.29.4",
        "ngx-moment": "^6.0.2",
        "rxjs": "^7.6.0",
        "zone.js": "^0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": ">=15.0.4",
        "@angular/cli": ">=15.0.4",
        "@angular/compiler-cli": ">=15.0.4",
        "@types/fhir": "^0.0.35",
        "@types/node": ">=18.11.15",
        "typescript": "^4.8.4",
        "webpack-bundle-analyzer": "^4.7.0"
    }
}